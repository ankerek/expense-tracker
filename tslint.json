{
  "extends": [
    "tslint:recommended",
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-console": false,
    "member-access": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-string-literal": false,
    "ban-types": false,
    "max-classes-per-file": false,
    "interface-name": [true, "never-prefix"]
  }
}
